package unnormal_arry;

public class arry1 {

	public static void main(String[] args) {
		//创建不规则数组第一种方式
		int [][] n1={
				{1,2,3},
				{6,58,45,4,5},
				{55},
				{45,78,45,24}
		};
		System.out.println("遍历n1");
		for(int i=0;i<n1.length;i++){
			for(int j=0;j<n1[i].length;j++){
				System.out.print(n1[i][j]+" ");
			}
			System.out.println();
		}
	}
}
