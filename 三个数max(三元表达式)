package dd;

import java.util.Scanner;

public class swq {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner sc=new Scanner(System.in);
    	int x=sc.nextInt();
		int y=sc.nextInt();
		int z=sc.nextInt();
		int max=(x>y)?(x>z?x:z):(y>z?y:z);
		System.out.println(max);
		
	}

}
